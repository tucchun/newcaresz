              <div class="pull-right">{{= it.examAuxiliaryUrineBld || ''}}</div>
            </div>
            <div class="item clearfix">
              <div class="pull-left">其他</div>
              <div class="pull-right">{{= it.examAuxiliaryUrineOther || ''}}</div>
            </div>
          </div>
        </div>
