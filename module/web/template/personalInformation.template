          }
          if(exposureHistory.expoPoison === 1){
            arr.push("毒物");
          }
          if(exposureHistory.expoRadial === 1){
            arr.push("射线");
          }

        %>
        <%= arr.join("、")%>
        <%
        }else{
        %>
            无
        <%
        }
        %>
      </td>
    </tr>
