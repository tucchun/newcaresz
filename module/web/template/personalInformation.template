            if(drupAllergy.drugPenicillin === 1){
        %>
              青霉素
        <%
            }
            if(drupAllergy.drugSulfa === 1){
        %>
              磺胺
        <%
            }
            if(drupAllergy.drugStreptomycin === 1){
        %>
             链霉素
        <%
            }
            if(drupAllergy.drugOtherSelect === 1){
        %>
              <%= drupAllergy.drugOther%>
        <%
            }
          }else{
        %>
            无
        <%
          }
        %>
