        }else if(residentMaritalStatus === 3){
        %>
        丧偶
        <%
        }else if(residentMaritalStatus === 4){
        %>
        离婚
        <%
        }else if(residentMaritalStatus === 5){
        %>
        未说明的婚姻状况
        <%
        }
        %>
